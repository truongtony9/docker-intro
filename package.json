{
  "name": "docker-intro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "NODE_ENV=development concurrently --kill-others \"react-scripts start\"  \"nodemon server/index.js\"",
